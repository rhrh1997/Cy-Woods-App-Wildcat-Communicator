    <script language="javascript" type="text/javascript">        
        var currentFadePercentage = 100;
        var currentPictureIndex = 0;
        function setOpacity(obj, opacity) 
        {
            opacity = (opacity == 100)?99.999:opacity;
            // IE/Win
            obj.style.filter = "alpha(opacity:"+opacity+")";

            // Safari<1.2, Konqueror
            obj.style.KHTMLOpacity = opacity/100;

            // Older Mozilla and Firefox
            obj.style.MozOpacity = opacity/100;

            // Safari 1.2, newer Firefox and Mozilla, CSS3
            obj.style.opacity = opacity/100;
        }    

        function FadeInImage()
        {
            var objFadeImage =document.getElementById('ctl00_cphMain_imgUserPic');
            if(currentFadePercentage<100)
            {
                currentFadePercentage+=10;
                setOpacity(objFadeImage, currentFadePercentage);
                setTimeout('FadeInImage()', 1);
            }
            else
            {
                var fadingElement = document.getElementById('ctl00_cphMain_lblPictureCounting').getElementsByTagName('a');
                for(var i=0;i<fadingElement.length;i++)
                {
                    fadingElement[i].setAttribute('clickEnable', 1);
                }                                            
                
                if(document.getElementById('ctl00_cphMain_dvNextPrev'))
                {
                    var dvNextPrevElement = document.getElementById('ctl00_cphMain_dvNextPrev').getElementsByTagName('a');
                    for(var j=0;j<dvNextPrevElement.length;j++)
                    {
                        dvNextPrevElement[j].setAttribute('clickEnable', 1);
                    }           
                }
            }
        }


        function FadeOutImage(imageSrc)
        {
            var objFadeImage =document.getElementById('ctl00_cphMain_imgUserPic');
            if(currentFadePercentage>0)
            {
                currentFadePercentage = currentFadePercentage - 10;
                setOpacity(objFadeImage, currentFadePercentage);                
                setTimeout('FadeOutImage("'+ imageSrc +'")' , 1);
            }
            else
            {
                setTimeout('ChangeImage("' + imageSrc + '")', 100);
            }
            return false;
        }
    
    
        function generateRSS()
        {
            var objEpsilenNewsRSSPopup = openPopupWindowwithObject('http://www.epsilen.com/Myportal/public/NewsFeedRss.aspx?RequestType=latest&prefix=timothykrae','News_Feed', 600, 600, 'scrollbars=yes,resizable=yes');
        }        
        
        function OpenWindow(URL)
        {
            return openPopupWindow(URL,'ViewVedio', 600, 800, 'scrollbars=yes,resizable=yes');
        }
        
        function ValidateForm()
        {
            var errorMsg="";
            var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
            if(document.getElementById('ctl00_cphMain_txtName').value=="")
            errorMsg+=' -Your name is required.' + "\n";
            if(document.getElementById('ctl00_cphMain_txtMessage').value=="")
            errorMsg+= '- Message is required.' + "\n";
             if(document.getElementById('ctl00_cphMain_txtEmail').value=="")
            errorMsg+='- Email is required' +"\n";
             if((reg.test(document.getElementById('ctl00_cphMain_txtEmail').value) == false)&&(document.getElementById('ctl00_cphMain_txtEmail').value.length>0))
             errorMsg+='- Email is not valid';
            if(errorMsg!="")
            {
                alert(errorMsg);
                return false;
            }
            else
            {
                 __doPostBack('ctl00$cphMain$btnSend','');
                return true;
            }
        }
        
        function ChangeImage(imageSrc)
        {
            var imgUserPic = document.getElementById('ctl00_cphMain_imgUserPic');
            imgUserPic.src = imageSrc;
            imgUserPic.style.visibility = 'hidden';
            setOpacity(imgUserPic, 0)
            imgUserPic.style.visibility = 'visible';            
            setTimeout('FadeInImage()' , 1);            
            return false;
        }
                
        function fadeImageOut(imageSrc, objClicked)
        {            
            if(objClicked.getAttribute('clickEnable')=='1')
            {
                var fadingElement = document.getElementById('ctl00_cphMain_lblPictureCounting').getElementsByTagName('a');
                for(var i=0;i<fadingElement.length;i++)
                {
                    fadingElement[i].setAttribute('clickEnable', 0);
                }                
                
                if(document.getElementById('ctl00_cphMain_dvNextPrev'))
                {
                    var dvNextPrevElement = document.getElementById('ctl00_cphMain_dvNextPrev').getElementsByTagName('a');
                    for(var i=0;i<dvNextPrevElement.length;i++)
                    {
                        dvNextPrevElement[i].setAttribute('clickEnable', 0);
                    }
                }           
                           
                currentFadePercentage=100;
                FadeOutImage(imageSrc);
                if(objClicked.getAttribute("pictureindex") != null)
                    currentPictureIndex = parseInt(objClicked.getAttribute("pictureindex"));
                return false;
            }
            else
            {
                alert('Please wait for the image to display');            
            }
        }        
        
        function imageProcessPrev(imgCntrol)
        {   
            if(imgCntrol.getAttribute("clickEnable")== '0')
            {
                alert('Please wait for the image to display');            
            }         
            if(currentPictureIndex > 0 && imgCntrol.getAttribute("clickEnable")== '1')
            {
                var imgSRC = imgCntrol.getAttribute("CSVImageSRC").split(',');                        
                currentPictureIndex -= 1;            
                fadeImageOut(imgSRC[currentPictureIndex], imgCntrol)
            }
            else
            {
                alert('You are viewing the first image')
            }            
        }
        
        function imageProcessNext(imgCntrol)
        {
            if(imgCntrol.getAttribute("clickEnable")== '0')
            {
                alert('Please wait for the image to display');            
            }
            var imgSRC = imgCntrol.getAttribute("CSVImageSRC").split(',');            
            if(currentPictureIndex < imgSRC.length - 1 && imgCntrol.getAttribute("clickEnable")== '1')
            {                
                currentPictureIndex += 1;               
                fadeImageOut(imgSRC[currentPictureIndex], imgCntrol)                
            }
            else
                alert('You are viewing the last image')
        }
    </script>

