        var baseURL = 'http://www.epsilen.com';
        var isPreview = "0"
        var accessKey='';
        var icon_root_path='http://www.epsilen.com/Images/Icons/';
        icon_root_path=icon_root_path.replace('~/',baseURL + '/');
        var tree_root_path='http://www.epsilen.com/Images/csh_vista';
        tree_root_path=tree_root_path.replace('~/',baseURL + '/');
        
        function chkAccessKey()
        {
            alert('Invalid access key.');
            return false;
        }
        
        function changeAccessKey(pAccessKey)
        {
        }
        
        function showDefaultIcon(obj)
        {
            obj.src=baseURL + '/images/Icons/CustomMenuIcons/icon_MenuComputer.gif';
        }
        
        function onImageMissingHeader(obj)
        {
            obj.src= baseURL + '/images/Epsilen_Logo_White_Small.gif';
        }
        
        function openPopup(obj, height, width)
        {
            var path = obj.getAttribute('popupPath');
            openPopupWindow(path, 'PERfileFolderInfo', height,width);
        }
        
        function quickLinkClick()
        {   
            if(readCookie("Epsilen_PER_Public") != null)
            {
                createCookie("Epsilen_PER_Public", (readCookie("Epsilen_PER_Public") == "Closed" ? "Open" : "Closed") , 0);
            }
            else
            {
                createCookie("Epsilen_PER_Public", "Closed" , 0);
            }
        }
    
        function changeClass2(divId)
        {
            mainMenu = document.getElementById('lnb_container');
		    thisOne = document.getElementById(divId);
		    items = mainMenu.getElementsByTagName("h3");
		    for(var i=0; i<items.length;i++)
		    {	
			    var itemId=items[i].id;
	            if(document.getElementById(itemId).className=="h3" || document.getElementById(itemId).className=="selected" ||document.getElementById(itemId).className=="h3 selected" )
	            {
		            document.getElementById(itemId).className="h3";
	            }
	            else
	            {
		            thisOne.className="selected";
	            }
	        }		
            document.getElementById(divId).className="h3 selected";
        }
        
        function quickLinkClick()
        {   
            if(readCookie("Epsilen_PER_Public") != null)
            {
                createCookie("Epsilen_PER_Public", (readCookie("Epsilen_PER_Public") == "Closed" ? "Open" : "Closed") , 0);
            }
            else
            {
                createCookie("Epsilen_PER_Public", "Closed" , 0);
            }
        }
        
        function click(e) 
        {
            alert('You are viewing this ePortfolio in "Preview" mode. This means that all links, buttons, and other options are disabled. This mode is for viewing how an ePortfolio will look and cannot be used to access any tools, options, or other pages.');
            return false;
        }
        
        function SubmiAccessKey()
        {
            WebForm_DoPostBackWithOptions(new WebForm_PostBackOptions("ctl00$btnGo", "", true, "", "", false, false))
        }
        
        function ValidateAccessKey()
        {
            if(document.getElementById('ctl00_txtAccessKey').value.length<=0)
            {
                alert('- Access key is required.');
                return false;
            }
            __doPostBack('ctl00$btnGoNew','')
        }
        
        function onAccessKeyPress(e)
        {
            var key;
            if(window.event)
                key = window.event.keyCode;
            else
                key = e.which;
                  
            if(key==13)
            {
                ValidateAccessKey();
                return false;
            }
            return true;
        }
        
        function onBodyLoad()
        {
            if(readCookie("Epsilen_PER_Public") == null)
            {
                setTimeout('extendContract()',1000);
            }
            else if(readCookie("Epsilen_PER_Public") == "Open")
            {
                setTimeout('extendContract()',1000);
            }
            
            var maxW = 530;
            if(document.getElementById("dvContainer"))
            {
                var pix=document.getElementById("dvContainer").getElementsByTagName('img');
                for (i=0; i<pix.length; i++) {
                    w=pix[i].width;
                    h=pix[i].height;
                    if (w > maxW) {
		                f=1-((w - maxW) / w);
                        pix[i].width=w * f;
                        pix[i].height=h * f;
		                if(pix[i].width == w)
			                pix[i].style.display = "none";
                    }
                }
            }
            changeAccessKey(accessKey);
        }
        
        function toggleChannels(ctrl)
        {
            if(document.getElementById(ctrl))
            {
                if(document.getElementById(ctrl).style.display == "")
                {
                    document.getElementById(ctrl).style.display = "none";
                }
                else
                {
                    document.getElementById(ctrl).style.display = "";
                }
            }
        }
        
