        var from_time = new Date();
        from_time = from_time.getTime();

        function show_loading_time()
        {
            var to_time = new Date();
            to_time = to_time.getTime();
            var secs = (to_time - from_time) /1000;
            document.getElementById("loadTime").innerHTML = "It took <b>"+secs+"</b> seconds to load this page.";
        }
        
        function logoMissing(ctrl)
        {
            var themPath = document.getElementById('ctl00_lnkPublicTheme').href + '';
            var logoPath = themPath.substr(0,themPath.lastIndexOf("/")) + '/images/epsilenLogo190x90.png';
            ctrl.src = logoPath;
        }

        width = 190;
        function HideContentDIV() {
            if (document.getElementById("dvPersonalPublicContentArea")) {
                document.getElementById("dvPersonalPublicContentArea").style.width = "744px";
            }
            document.getElementById("sideBarContents").style.display = "none";
        }
