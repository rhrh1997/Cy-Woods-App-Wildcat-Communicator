    <script language="javascript" type="text/javascript">
        var baseURL = 'http://www.epsilen.com';
        function onImageMissing(obj)
        {
            var rootPath = baseURL + '/images/noMembersBig.jpg';
            obj.src=rootPath; 
        }
        function openAlbumViewer(contrl)
        {
            var urlForAlbumViewer = contrl.getAttribute("URLForAlbumViewer");
            return openPopupWindow(urlForAlbumViewer,'AlbumViewer',650, 650, '');
        }
